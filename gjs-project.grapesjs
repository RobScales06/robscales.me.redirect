ssList.remove(\"block\");\n              el.classList.add(\"hidden\");\n            }\n          });\n          var links = document.querySelectorAll('nav a[href^=\"#\"]');\n          links.forEach(function(a) {\n            if (a.getAttribute(\"href\") === \"#\" + hash) {\n              a.classList.add(\"ring-1\", \"ring-white/30\");\n            } else {\n              a.classList.remove(\"ring-1\", \"ring-white/30\");\n            }\n          });\n          window.scrollTo({\n            top: 0,\n            behavior: \"smooth\"\n          });\n        }\n        window.addEventListener(\"hashchange\", setRouteFromHash);\n        document.addEventListener(\"DOMContentLoaded\", function() {\n          setRouteFromHash();\n          var audio = document.querySelector('audio');\n          var source = audio.querySelector('source');\n          var titleEl = document.querySelector('[data-gjs-name=\"Player Track Title\"]');\n          var subtitleEl = document.querySelector('[data-gjs-name=\"Player Track Subtitle\"]');\n          var items = Array.prototype.slice.call(document.querySelectorAll('[data-gjs-name=\"Playlist Play Button\"]'));\n          var tracks = items.map(function(btn) {\n            return {\n              title: btn.parentElement.querySelector('[data-gjs-name=\"Playlist Item Title\"]').textContent,\n              url: btn.getAttribute('data-track'),\n              button: btn\n            };\n          });\n          var current = -1;\n\n          function playIndex(i) {\n            if (i < 0 || i >= tracks.length) return;\n            current = i;\n            source.src = tracks[i].url;\n            audio.load();\n            audio.play();\n            titleEl.textContent = tracks[i].title;\n            subtitleEl.textContent = \"Now Playing\";\n            items.forEach(function(b) {\n              b.textContent = \"Play\";\n            });\n            tracks[i].button.textContent = \"Playing\";\n          }\n          items.forEach(function(btn, idx) {\n            btn.addEventListener(\"click\", function() {\n              playIndex(idx);\n            });\n          });\n          document.getElementById(\"musicPrev\").addEventListener(\"click\", function() {\n            if (current <= 0) playIndex(tracks.length - 1);\n            else playIndex(current - 1);\n          });\n          document.getElementById(\"musicNext\").addEventListener(\"click\", function() {\n            if (current >= tracks.length - 1) playIndex(0);\n            else playIndex(current + 1);\n          });\n        });\n      })();\n    "}]}],"doctype":"<!DOCTYPE html>","head":{"type":"head","components":[{"tagName":"title","type":"text","components":[{"type":"textnode","content":"Portfolio | Art  Animation  Games  Music  Software"}]},{"tagName":"meta","void":true,"attributes":{"charset":"utf-8"}},{"tagName":"meta","void":true,"attributes":{"name":"viewport","content":"width=device-width, initial-scale=1"}},{"tagName":"meta","void":true,"attributes":{"name":"robots","content":"index,follow"}},{"tagName":"meta","void":true,"attributes":{"name":"generator","content":"GrapesJS Studio"}},{"tagName":"link","void":true,"attributes":{"href":"https://fonts.googleapis.com/css2?family=Inter:wght@400;500;600;700&family=Poppins:wght@600;700&display=swap","rel":"stylesheet"}}]},"docEl":{"tagName":"html","attributes":{"lang":"en"}}},"id":"YvyxKE5oabkBhQ2w","minHeight":"500px"}],"id":"bpCSSgQnLbZcgo2j"},{"name":"Art","frames":[{"component":{"type":"wrapper","name":"Body","classes":["gjs-t-body","body"],"attributes":{"id":"ic9erg"},"components":[{"name":"Banner Container","classes":["banner-container"],"attributes":{"id":"iggb9xk"},"components":[{"name":"Banner Row","classes":["banner-row"],"components":[{"name":"Banner Left Image Wrap","classes":["banner-left-image-wrap"],"components":[{"type":"image","name":"Banner Left Image","resizable":{"ratioDefault":1},"classes":["banner-left-image"],"attributes":{"alt":"Left Banner Image","loading":"eage